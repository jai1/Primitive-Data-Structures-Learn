package org.learn.interview;

import static org.junit.Assert.*;

import org.junit.Before;
import org.junit.Test;

public class LinkedListTest {

	LinkedList.Node n = null;
	@Before
	public void setUp() throws Exception {
		n = new LinkedList.Node(10);
	}

	@Test
	public void test() {
		n.nextNode = new LinkedList.Node(10);
		n.nextNode.nextNode = new LinkedList.Node(10);
		n.nextNode.nextNode.nextNode = new LinkedList.Node(10);
		n.nextNode.nextNode.nextNode.nextNode = new LinkedList.Node(10);
		n = LinkedList.removeDuplicates(n);
		assertEquals(LinkedList.toString(n) , "10");
	}

}
